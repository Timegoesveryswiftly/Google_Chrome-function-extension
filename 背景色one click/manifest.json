{
    "name": "My Third Extension",
    "version": "0.1",
    "manifest_version": 3,

    "description": "今いるpageの背景色をone click変化！",

    "permissions": [
      "tabs", "http://*/*"
    ],

    "background": {
      "scripts": ["background.js"]
    },

    "browser_action": {
      "default_icon": "ea1.png",
      "default_title": "My Third Extention"
    }
}
